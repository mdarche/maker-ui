{"version":3,"sources":["../../src/map/keycode.js"],"names":["keycode","tab","left","up","right","down","pageUp","pageDown","end","home","enter","escape","space","shift","capsLock","ctrl","alt","meta","pause","insert","backspace","_alias","n","code","numCode","name","String","fromCharCode","toLowerCase"],"mappings":";;;;;;AACA;AACA;AACA;AACA;AACA;;AAEA,IAAMA,UAAU;AACd;AACAC,OAAK,CAFS;;AAId;AACAC,QAAM,EALQ;AAMdC,MAAI,EANU;AAOdC,SAAO,EAPO;AAQdC,QAAM,EARQ;AASdC,UAAQ,EATM;AAUd,aAAW,EAVG;AAWdC,YAAU,EAXI;AAYd,eAAa,EAZC;AAadC,OAAK,EAbS;AAcdC,QAAM,EAdQ;;AAgBd;AACAC,SAAO,EAjBO;AAkBdC,UAAQ,EAlBM;AAmBdC,SAAO,EAnBO;;AAqBd;AACAC,SAAO,EAtBO;AAuBdC,YAAU,EAvBI;AAwBd,eAAa,EAxBC;AAyBdC,QAAM,EAzBQ;AA0BdC,OAAK,EA1BS;AA2BdC,QAAM,EA3BQ;AA4Bd;AACA;AACA;AACAC,SAAO,EA/BO;;AAiCd;AACAC,UAAQ,EAlCM;AAmCd,YAAU,EAnCI;AAoCdC,aAAW,CApCG;;AAsCd;AACAC,UAAQ;AACN,QAAI,CAAC,EAAD,EAAK,EAAL,EAAS,GAAT;AADE;AAvCM,CAAhB;;AA4CA;AACA;AACA,KAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAI,EAApB,EAAwBA,GAAxB,EAA6B;AAC3BtB,UAAQ,MAAMsB,CAAd,IAAmBA,IAAI,GAAvB;AACD;;AAED;AACA;AACA,KAAK,IAAIA,KAAI,CAAb,EAAgBA,KAAI,EAApB,EAAwBA,IAAxB,EAA6B;AAC3B,MAAMC,OAAOD,KAAI,EAAjB;AACA,MAAME,UAAUF,KAAI,EAApB;AACAtB,UAAQsB,EAAR,IAAaC,IAAb;AACAvB,UAAQ,SAASsB,EAAjB,IAAsBE,OAAtB;AACAxB,UAAQqB,MAAR,CAAeE,IAAf,IAAuB,CAACC,OAAD,CAAvB;AACD;;AAED;AACA,KAAK,IAAIF,MAAI,CAAb,EAAgBA,MAAI,EAApB,EAAwBA,KAAxB,EAA6B;AAC3B,MAAMC,QAAOD,MAAI,EAAjB;AACA,MAAMG,OAAOC,OAAOC,YAAP,CAAoBJ,KAApB,EAA0BK,WAA1B,EAAb;AACA5B,UAAQyB,IAAR,IAAgBF,KAAhB;AACD;;kBAEcvB,O","file":"keycode.js","sourcesContent":["\n// codes mostly cloned from https://github.com/keithamus/jwerty/blob/master/jwerty.js\n// deliberately not exposing characters like <,.-#* because they vary *wildly*\n// across keyboard layouts and may cause various problems\n// (e.g. \"*\" is \"Shift +\" on a German Mac keyboard)\n// (e.g. \"@\" is \"Alt L\" on a German Mac keyboard)\n\nconst keycode = {\n  // Element Focus\n  tab: 9,\n\n  // Navigation\n  left: 37,\n  up: 38,\n  right: 39,\n  down: 40,\n  pageUp: 33,\n  'page-up': 33,\n  pageDown: 34,\n  'page-down': 34,\n  end: 35,\n  home: 36,\n\n  // Action\n  enter: 13,\n  escape: 27,\n  space: 32,\n\n  // Modifier\n  shift: 16,\n  capsLock: 20,\n  'caps-lock': 20,\n  ctrl: 17,\n  alt: 18,\n  meta: 91,\n  // in firefox: 224\n  // on mac (chrome): meta-left=91, meta-right=93\n  // on win (IE11): meta-left=91, meta-right=92\n  pause: 19,\n\n  // Content Manipulation\n  insert: 45,\n  'delete': 46,\n  backspace: 8,\n\n  // the same logical key may be identified through different keyCodes\n  _alias: {\n    91: [92, 93, 224],\n  },\n};\n\n// Function keys (112 - 137)\n// NOTE: not every keyboard knows F13+\nfor (let n = 1; n < 26; n++) {\n  keycode['f' + n] = n + 111;\n}\n\n// Number keys (48-57, numpad 96-105)\n// NOTE: not every keyboard knows num-0+\nfor (let n = 0; n < 10; n++) {\n  const code = n + 48;\n  const numCode = n + 96;\n  keycode[n] = code;\n  keycode['num-' + n] = numCode;\n  keycode._alias[code] = [numCode];\n}\n\n// Latin characters (65 - 90)\nfor (let n = 0; n < 26; n++) {\n  const code = n + 65;\n  const name = String.fromCharCode(code).toLowerCase();\n  keycode[name] = code;\n}\n\nexport default keycode;\n"]}