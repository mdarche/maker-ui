{"version":3,"sources":["../../src/util/tabindex-value.js"],"names":["element","hasTabindex","hasAttribute","attributeName","tabindex","parseInt","getAttribute","isNaN"],"mappings":";;;;;;;;;;;;;;;;;;;oBAGe,UAASA,OAAT,EAAkB;AAC/B,QAAI,CAAC,6BAAgBA,OAAhB,CAAL,EAA+B;AAC7B,aAAO,IAAP;AACD;;AAED;AACA;AACA,QAAMC,cAAcD,QAAQE,YAAR,CAAqB,UAArB,CAApB;AACA,QAAMC,gBAAgBF,cAAc,UAAd,GAA2B,UAAjD;;AAEA;AACA,QAAMG,WAAWC,SAASL,QAAQM,YAAR,CAAqBH,aAArB,CAAT,EAA8C,EAA9C,CAAjB;AACA,WAAOI,MAAMH,QAAN,IAAkB,CAAC,CAAnB,GAAuBA,QAA9B;AACD,G","file":"tabindex-value.js","sourcesContent":["\nimport isValidTabindex from '../is/valid-tabindex';\n\nexport default function(element) {\n  if (!isValidTabindex(element)) {\n    return null;\n  }\n\n  // Edge 14 has a capitalization problem on SVG elements,\n  // see https://developer.microsoft.com/en-us/microsoft-edge/platform/issues/9282058/\n  const hasTabindex = element.hasAttribute('tabindex');\n  const attributeName = hasTabindex ? 'tabindex' : 'tabIndex';\n\n  // @browser-issue Gecko https://bugzilla.mozilla.org/show_bug.cgi?id=1128054\n  const tabindex = parseInt(element.getAttribute(attributeName), 10);\n  return isNaN(tabindex) ? -1 : tabindex;\n}\n"]}