{
  "private": true,
  "scripts": {
    "clean": "turbo run clean",
    "git-reset": "git reset --hard HEAD",
    "docs:dev": "pnpm recursive run dev --filter docs",
    "docs:build": "pnpm recursive run build --filter docs",
    "docgen": "turbo run docgen --parallel",
    "postdocgen": "rm -rf apps/docs/temp",
    "start": "turbo run start --parallel --continue",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "build": "turbo run build --filter=*maker-ui* --no-deps --include-dependencies",
    "cy:open": "cypress open --component",
    "cy:run": "cypress run --component",
    "postinstall": "pnpm build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.11.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-test-renderer": "^17.0.0",
    "babel-jest": "^29.4.3",
    "cypress": "^12.17.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.44.0",
    "eslint-config-app": "workspace:*",
    "husky": "^7.0.1",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.4.3",
    "jsdom": "^21.1.0",
    "next": "^13.4.10",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.6.0",
    "tsup": "^7.1.0",
    "turbo": "latest",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@8.6.7",
  "pnpm": {
    "overrides": {
      "esbuild": "^0.18.2"
    }
  }
}
