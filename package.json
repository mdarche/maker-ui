{
  "private": true,
  "scripts": {
    "clean": "turbo run clean",
    "git-clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "git-reset": "git reset --hard HEAD",
    "docs-start": "yarn workspace docs dev",
    "start-old": "lerna run start --stream --parallel",
    "start": "turbo run dev --parallel --continue",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "build": "turbo run build",
    "prepublish": "lerna run prepublish",
    "cy": "cypress open",
    "cy:headless": "cypress run",
    "cy:unit": "cypress open-ct",
    "cy:unit:headless": "cypress run-ct",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "workspaces": [
    "examples/*",
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.5",
    "@cypress/react": "^5.4.2",
    "@cypress/webpack-dev-server": "^1.4.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^17.0.0",
    "@weiran.zsd/tsdx": "^0.15.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^9.3.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.10.0",
    "html-webpack-plugin": "4",
    "husky": "^7.0.1",
    "lerna": "^3.20.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "tsdx": "https://github.com/weiran-zsd/tsdx.git",
    "turbo": "^1.1.5"
  }
}
