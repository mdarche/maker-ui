{
  "private": true,
  "scripts": {
    "clean": "turbo run clean",
    "git-reset": "git reset --hard HEAD",
    "docs:dev": "pnpm recursive run dev --filter docs",
    "docs:build": "pnpm recursive run build --filter docs",
    "e2e:dev": "pnpm recursive run dev --filter e2e",
    "e2e:build": "pnpm recursive run build --filter e2e",
    "docgen": "turbo run docgen --parallel",
    "postdocgen": "rm -rf apps/docs/temp",
    "start": "turbo run start --parallel --continue",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "build": "turbo run build --scope=*maker-ui* --no-deps --include-dependencies",
    "cy": "cypress open",
    "cy:headless": "cypress run",
    "cy:unit": "cypress open-ct",
    "cy:unit:headless": "cypress run-ct",
    "postinstall": "pnpm build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "workspaces": [
    "examples/*",
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@cypress/code-coverage": "^3.9.5",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.2",
    "@types/node": "^17.0.21",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^17.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^9.5.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "7.32.0",
    "html-webpack-plugin": "5",
    "husky": "^7.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "tsconfig-paths": "^3.13.0",
    "turbo": "^1.1.5",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2",
    "webpack": "5",
    "webpack-dev-server": "3"
  },
  "packageManager": "pnpm@6.32.3"
}
