{
  "private": true,
  "scripts": {
    "clean": "turbo run clean",
    "git-reset": "git reset --hard HEAD",
    "docs:dev": "pnpm recursive run dev --filter docs",
    "docs:build": "pnpm recursive run build --filter docs",
    "docgen": "turbo run docgen --parallel",
    "postdocgen": "rm -rf apps/docs/temp",
    "start": "turbo run start --parallel --continue",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "build": "turbo run build --filter=*maker-ui* --no-deps --include-dependencies",
    "cy:open": "cypress open --component",
    "cy:run": "cypress run --component",
    "postinstall": "pnpm build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^17.0.0",
    "cypress": "^12.6.0",
    "eslint": "7.32.0",
    "eslint-config-app": "workspace:*",
    "husky": "^7.0.1",
    "next": "^13.1.6",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.1",
    "turbo": "latest",
    "typedoc": "^0.22.13",
    "typescript": "^4.8.4"
  },
  "packageManager": "pnpm@7.1.7"
}
