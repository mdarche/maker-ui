{
  "name": "@maker-ui/elements",
  "description": "Customizable components for Maker UI themes.",
  "version": "1.1.2",
  "license": "Apache-2.0",
  "author": "Mike Darche <mike@newcreative.io>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "docgen": "npx typedoc --tsconfig tsconfig.docs.json",
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "start": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "maker-ui": "1.1.2"
  },
  "devDependencies": {
    "@types/react-transition-group": "^4.4.4",
    "tsconfig": "*",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2"
  }
}
