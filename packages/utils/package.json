{
  "name": "@maker-ui/utils",
  "description": "Utility functions and react hooks that are used throughout Maker UI libraries",
  "version": "1.2.5",
  "license": "Apache-2.0",
  "author": "Mike Darche <mike@newcreative.io>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "start": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "test": "jest",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "deepmerge": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^17.x || ^18.x",
    "react-dom": "^17.x || ^18.x"
  },
  "devDependencies": {
    "eslint-config-library": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^5.12.0",
    "typescript": "^4.8.4"
  }
}
