    "maker-ui": "2.0.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.6",
    "eslint": "7.32.0",
    "eslint-preset": "*",
    "gsap": "^3.9.1",
    "tsconfig": "*",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "lodash.debounce": "^4.0.8"
  },
  "peerDependenciesMeta": {
