  "author": "Mike Darche <mike@newcreative.io>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "docgen": "npx typedoc --tsconfig tsconfig.docs.json",
