{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["es2015", "dom"],
    "module": "esnext",
    "target": "es6",
    "jsx": "react-jsx"
  },
  "typedocOptions": {
    "out": "apps/docs/temp",
    "excludeExternals": true,
    "excludeInternal": true,
    "pretty": true,
    "emit": "docs"
  }
}
