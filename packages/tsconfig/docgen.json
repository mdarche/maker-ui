{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Typedoc Generator",
  "extends": "./react-library.json",
  "compilerOptions": {
    "skipLibCheck": true
  },
  "typedocOptions": {
    "out": "apps/docs/temp",
    "excludeExternals": true,
    "excludeInternal": true,
    "pretty": true,
    "emit": "docs"
  }
}
