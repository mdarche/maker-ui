  "dependencies": {
    "@emotion/react": "11.8.2",
    "@maker-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "@emotion/jest": "^11.8.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.1",
    "eslint": "7.32.0",
    "eslint-preset": "workspace:*",
    "react-test-renderer": "^17.0.1",
    "tsconfig": "workspace:*",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2"
  }
}
