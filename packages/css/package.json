  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "docgen": "npx typedoc --tsconfig tsconfig.docs.json",
