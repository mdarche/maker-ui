{
  "name": "@maker-ui/layout",
  "description": "The fastest way to build themeable, responsive layouts in React",
  "version": "1.1.2",
  "license": "Apache-2.0",
  "author": "Mike Darche <mike@newcreative.io>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "start": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@juggle/resize-observer": "^3.3.1",
    "@maker-ui/css": "1.1.2",
    "@maker-ui/primitives": "1.1.2",
    "deepmerge": "^4.2.2",
    "react-use-measure": "^2.0.4"
  },
  "devDependencies": {
    "tsconfig": "*",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2"
  }
}
