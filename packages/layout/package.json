{
  "name": "@maker-ui/layout",
  "description": "The fastest way to build themeable, responsive layouts in React",
  "version": "1.2.5",
  "license": "Apache-2.0",
  "author": "Mike Darche <mike@newcreative.io>",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./layout.css": "./dist/index.css",
    "./client.css": "./dist/client.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "lint": "eslint src",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@maker-ui/hooks": "workspace:*",
    "@maker-ui/style": "workspace:*",
    "@maker-ui/utils": "workspace:*"
  },
  "peerDependencies": {
    "next": "^13.x",
    "react": "^17.x || ^18.x",
    "react-dom": "^17.x || ^18.x"
  },
  "devDependencies": {
    "@maker-ui/build-tools": "workspace:*",
    "eslint-config-library": "workspace:*",
    "next": "^13.2.3",
    "postcss": "^8.4.18",
    "postcss-modules": "^5.0.0",
    "postcss-nested": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^4.8.4"
  }
}
